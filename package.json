{
  "name": "medsta",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "firebase:login": "firebase login",
    "deploy:rules": "firebase deploy --only firestore:rules"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "firebase": "^12.4.0",
    "leaflet": "^1.9.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0",
    "react-leaflet": "^5.0.0",
    "react-responsive-carousel": "^3.2.23",
    "react-router-dom": "^7.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.4.2",
    "vite": "^7.1.7"
  },
  "rules_version": "2",
  "service": "cloud.firestore",
  "match": {
    "/databases/{database}/documents": {
      "function": "isOwner",
      "allow": {
        "read": "request.auth != null && request.auth.uid == request.resource.id",
        "write": "request.auth != null && request.auth.uid == request.resource.id"
      }
    },
    "/users/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    },
    "/patients/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    },
    "/providers_doctors/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    },
    "/providers_diagnostic_centers/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    },
    "/providers_pharmacies/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    },
    "/providers_delivery_agents/{userId}": {
      "allow": {
        "read": "request.auth != null && request.auth.uid == userId",
        "write": "request.auth != null && request.auth.uid == userId"
      }
    }
  }
}
